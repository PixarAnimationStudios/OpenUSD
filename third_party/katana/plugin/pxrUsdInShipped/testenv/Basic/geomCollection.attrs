<attr type="GroupAttr">
  <attr name="collections" type="GroupAttr">
    <attr name="atGeom" type="GroupAttr">
      <attr name="cel" tupleSize="1" type="StringAttr">
        <sample size="1" time="0">
          <str value="((/LampStem /LampStem//* ))"/>
        </sample>
      </attr>
    </attr>
    <attr name="ancestorAtGeom" type="GroupAttr">
      <attr name="cel" tupleSize="1" type="StringAttr">
        <sample size="1" time="0">
          <str value="(/root/world/geo/World/anim/TestCollections/$ancestor)"/>
        </sample>
      </attr>
    </attr>
    <attr name="atGeomAtGeom" type="GroupAttr">
      <attr name="cel" tupleSize="1" type="StringAttr">
        <sample size="1" time="0">
          <str value="(//$atGeom)"/>
        </sample>
      </attr>
    </attr>
    <attr name="descendantAtGeom" type="GroupAttr">
      <attr name="cel" tupleSize="1" type="StringAttr">
        <sample size="1" time="0">
          <str value="(/LampBase/$descendant)"/>
        </sample>
      </attr>
    </attr>
    <attr name="union" type="GroupAttr">
      <attr name="cel" tupleSize="1" type="StringAttr">
        <sample size="3" time="0">
          <str value="(/root/world/geo/World/anim/TestCollections/$ancestor)"/>
          <str value="(//$atGeom)"/>
          <str value="(/LampBase/$descendant)"/>
        </sample>
      </attr>
    </attr>
  </attr>
  <attr name="info" type="GroupAttr">
    <attr name="usd" type="GroupAttr">
      <attr name="apiSchemas" tupleSize="1" type="StringAttr">
        <sample size="5" time="0">
          <str value="CollectionAPI:atGeom"/>
          <str value="CollectionAPI:ancestorAtGeom"/>
          <str value="CollectionAPI:atGeomAtGeom"/>
          <str value="CollectionAPI:descendantAtGeom"/>
          <str value="CollectionAPI:union"/>
        </sample>
      </attr>
    </attr>
  </attr>
  <attr name="type" tupleSize="1" type="StringAttr">
    <sample size="1" time="0">
      <str value="group"/>
    </sample>
  </attr>
</attr>
