# Property tests.
def Scope "P"
{
    custom double b ("b")
    custom double c ("c")
    custom double g ("g")
    custom double h ("h")
    custom double i ("i")
    custom double x

    custom rel j
    add rel j = </S.j>
    custom rel k
    add rel k = </P.j>
}
def Scope "Q"
{
    def Scope "R"
    {
    }
}
def Scope "S"
{
    custom rel j
    custom rel k
}

# Variants.
def Scope "V" (
    variantSets = "v"
)
{
    variantSet "v" = {
        "one" {
            custom double u

            def Scope "U"
            {
            }
        }
        "two" {
            custom double w

            def Scope "W"
            {
            }
        }
    }
}

