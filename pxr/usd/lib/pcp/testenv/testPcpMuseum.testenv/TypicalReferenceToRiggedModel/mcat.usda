#usda 1.0

def "Model" (
    add inherits = </Class>
    add variantSets = "pin"
    variants = {
        string pin = "stable"
    }
)
{
    variantSet "pin" = {
        "stable" (
            references = @model_stable.usda@</Model>
        ) { }
        "latest" (
            references = @model_latest.usda@</Model>
        ) { }
    }
}

class "Class"
{
}

