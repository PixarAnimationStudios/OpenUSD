#usda 1.0
(
    endTimeCode = 104
    startTimeCode = 101
)

def Xform "World" (
    customData = {
        bool zUp = 1
    }
    kind = "group"
)
{
    def Xform "fx" (
        kind = "group"
    )
    {
        def Xform "Particles_Splash" (
            kind = "component"
        )
        {
            def Points "points"
            {
                float3[] extent
                point3f[] points
                custom int[] primvars:id (
                    elementSize = 1
                    interpolation = "varying"
                )
                vector3f[] velocities
                float[] widths
                custom matrix4d xformOp:transform
                uniform token[] xformOpOrder = ["xformOp:transform"]
            }
        }
    }
}

