#sdf 1.4.32

def "Model" (
    add inherits = </Class>
    add specializes = </Specializes>
    add variantSets = "pin"
    variants = {
        string pin = "stable"
    }
)
{
    variantSet "pin" = {
        "stable" (
            references = @./model_stable.sdf@</Model>
        ) { }
        "latest" (
            references = @./model_latest.sdf@</Model>
        ) { }
    }
}

class "Class"
{
}

class "Specializes"
{
}
