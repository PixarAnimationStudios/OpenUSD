#sdf 1.4.32

over "A" (
    '''This simply gives the order x,y,z.'''
    prepend inherits = </sub2_prepend>
    append inherits = </sub2_append>
)
{
    double x
    double y
    double z

    over "x" {
    }
    over "y" {
    }
    over "z" {
    }
}

class "sub2_prepend"
{
    prepend rel targets = </sub2_prepend/sub2_prepend>
}

class "sub2_append"
{
    prepend rel targets = </sub2_append/sub2_append>
}
